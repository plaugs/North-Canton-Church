<h1>Listing Images</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_images.each do |admin_image| %>
  <tr>
	  <td><%= image_tag admin_image.image.thumb.url %>
    <td><%= link_to 'Show', admin_image %></td>
    <td><%= link_to 'Edit', edit_admin_image_path(admin_image) %></td>
    <td><%= link_to 'Destroy', admin_image, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

